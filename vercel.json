{
    "versoin":2,
    "builds":[
        {
            "src":"ticketing_tool/wsgi.py",
            "use":"@vercel/python",
            "config":{"maxLambdaSize":"15mb","runtime":"Python3.12.2"}
        },
        {
            "src":"build_files.sh",
            "use":"@vercel/static-build",
            "config":{
                "distDir":"staticfiles_build"
            }
        }
    ],
    "routes":[
        {
            "src":"/static/(.*)",
            "dest":"/static/$1"
        },
        {
            "src":"/(.*)",
            "dest":"ticketing_tool/wsgi.py"
        }
    ]
}